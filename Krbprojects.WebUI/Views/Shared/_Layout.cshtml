                            let path = `${window.location.pathname}${window.location.search}`;
                            let selectlang = $(e.currentTarget).data('lang');
                            let result = /\/(az|en|ru)?\/?(.*)?/.exec(path);
                            if (result != null && result[2] != undefined) {
                                path = `/${selectlang}/${result[2]}`;
                            }
                            else if (result != null) {
                                path = `/${selectlang}`;
                            }
                            window.location.href = path;
                            console.log(path);
                            console.log(selectlang);
                        });
